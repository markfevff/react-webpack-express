{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "env": {
        "es6": true
    },
    "globals": {
        "console": "readonly",
        "setTimeout": "readonly",
        "setInterval": "readonly",
        "window": "readonly",
        "document": "readonly",
        "URLSearchParams": "readonly"
    },
    "rules": {
        "no-undef": "error",
        "no-use-before-define": "warn",
        "no-cond-assign": "error",
        "no-const-assign": "error",
        "no-dupe-class-members": "error",
        "constructor-super": "warn"
    }
}